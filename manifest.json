{
  "manifest_version": 2,

  "name": "Html Destroyer",
  "description": "A tool to test front end builds",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
    //   "css": ["mystyles.css"],
      "js": ["jquery-3.1.0.min.js", "myscript.js"]
    }
  ],  
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
